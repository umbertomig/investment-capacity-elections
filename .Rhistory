gstar1 <- function(R, theta) {
c1 = 9/(16*(R^2))
print(c1)
c2 = 2+R^2-8*R*theta
print(c2)
c3 = 2*sqrt(1-3*(R^2)+2*(R^3)-8*R*theta+16*(R^2)*theta - 8*(R^3)*theta )
print(c3)
g = c1*(c2 - c3)
t = 1/2+(2/3)*sqrt(g)
r = t*(3/4 - t + sqrt(g)) - g
return(c(g, t, r))
}
